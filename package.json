{
  "name": "file-metadata",
  "private": true,
  "version": "0.0.0",
  "description": "Free Code Camp - File Metadata Microservice",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src/**/*.js",
    "babel": "npm run -s lint && rimraf dist && babel src -d dist",
    "start": "node .",
    "start:dev": "babel-node src",
    "dev": "npm-watch"
  },
  "watch": {
    "start:dev": {
      "patterns": [
