    "babel-eslint": "6.0.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-2": "6.5.0",
    "babel-register": "6.8.0",
    "eslint": "2.9.0",
    "npm-watch": "0.1.4",
    "rimraf": "2.5.2"
  },
  "dependencies": {
    "babel-polyfill": "6.8.0",
    "express": "4.13.4",
    "express-async-wrap": "1.0.0",
    "jade": "1.11.0",
    "multer": "1.1.0"
